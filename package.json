{
  "name": "backend",
  "version": "1.0.0",
  "description": "The back-end server for the Labs16 Design Hub Project",
  "main": "index.js",
  "scripts": {
    "test": "cross-env DB_ENV=testing jest --watchAll --verbose --runInBand --forceExit",
    "test-redo": "npx knex migrate:rollback --env=testing && npx knex migrate:latest --env=testing && npx knex seed:run --env=testing",
    "dev": "nodemon index.js",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labs16-designhub/backend.git"
  },
  "keywords": [
    "DesignHub",
    "UX",
    "PostgreSQL",
    "Express",
    "UI"
  ],
  "author": "Cecil John Tantay, Javontay McElroy, Bradley Ball, Ian Belknap, Zeke Kooyer, Michael VanSleen, Landon Bassett",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/labs16-designhub/backend/issues"
  },
  "homepage": "https://github.com/labs16-designhub/backend#readme",
  "dependencies": {
    "@sentry/node": "5.6.2",
    "aws-sdk": "^2.528.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.0",
    "knex": "^0.19.4",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "cross-env": "^6.0.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "clearMocks": true,
    "displayName": "Design Hub X Server",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true
  }
}
